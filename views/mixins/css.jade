mixin css(moduleName)
  if (moduleName == 'grid')
    link(type="text/css", rel="stylesheet", href="stylesheets/grid/grid.css")
    
  else if (moduleName == 'tag')
    link(type="text/css", rel="stylesheet", href="stylesheets/tag.css")

  else if (moduleName == 'mainpage')
    link(type="text/css", rel="stylesheet", href="stylesheets/main.css")

  else if (moduleName == 'editor')
    link(rel="stylesheet", href="stylesheets/editor.css")